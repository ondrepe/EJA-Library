<?xml version="1.0" encoding="UTF-8"?>

<ejb-jar xmlns = "http://java.sun.com/xml/ns/javaee" 
         version = "3.1"
         xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation = "http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd">
    <enterprise-beans>
        <session>
            <ejb-name>ReadersAdministration</ejb-name>
            <ejb-class>cz.cvut.fel.x33eja.libejb.session.reader.ReadersAdministration</ejb-class>
            <env-entry>
                <env-entry-name>ejb/ReadersAdministration</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>lib-ejb-context.xml</env-entry-value>
            </env-entry>
        </session>
    </enterprise-beans>

</ejb-jar>
